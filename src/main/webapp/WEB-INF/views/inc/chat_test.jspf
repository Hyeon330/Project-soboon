<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<style>
#msgBtn {
	width: 40px;
	height: 40px;
	position: fixed;
	right: 30px;
	bottom: 0;
	background-color: #F29661;
	border: 0;
}
.bi-chat-square-text {
	font-size: 20px;
}
#msgPopup {
	width: 250px;
	height: 0;
	overflow: hidden;
	background-color: #fff
}
</style>
<script>
	$(() => {
		$('#msgBtn').click(function() {
			$('#messagePopup').css('display','flex');
		});
		$('.bi-x').click(function() {
			$('#messagePopup').css('display','none');
		});
	});
</script>
<c:if test="${logStatus!='Y'}">
	<button id="msgBtn"><i class="bi bi-chat-square-text"></i></button>
	<div id="msgPopup">
	</div>
</c:if>